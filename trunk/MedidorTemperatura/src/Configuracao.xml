<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   
    <class name="br.com.medidor.temperatura.bean.Configuracao" >
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="segundosAtualizacao" type="java.lang.Integer" />
        <property name="temperaturaMinima" type="java.lang.Integer" />
        <property name="temperaturaMaxima" type="java.lang.Integer" />
        <property name="data" type="java.util.Date" />
   
   </class>
</hibernate-mapping>
