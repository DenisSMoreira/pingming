<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cbss.bean.canonico.EmpresaParceira" table="TEMPRPCERA" >
        <id name="codigoEmpresa" type="java.lang.Long">
            <column name="CEMPRPCERA"  />
        </id>
        <property name="nomeEmpresa" type="java.lang.String">
            <column name="IEMPRPCERA"  />
        </property>
        <property name="numeroRaizCnpj" type="java.lang.Long">
            <column name="NRAIZCNPJ" />
        </property>
        <property name="numeroFilialCnpj" type="java.lang.Long">
            <column name="NFLIALCNPJ" />
        </property>
        <property name="numeroCnpj" type="java.lang.Long">
            <column name="NDVCNPJ" />
        </property>
        <property name="numeroDddFilial" type="java.lang.Long">
            <column name="NDDDFLIAL" />
        </property>
        <property name="numeroTelefoneFilial" type="java.lang.Long">
            <column name="NFONEFLIAL" />
        </property>
        <property name="numeroFaxFilial" type="java.lang.Long">
            <column name="NFAXFLIAL" />
        </property>
        <property name="numeroDddFaxFilial" type="java.lang.String">
            <column name="NDDDFAXFLIAL" />
        </property>
       
    </class>
</hibernate-mapping>
