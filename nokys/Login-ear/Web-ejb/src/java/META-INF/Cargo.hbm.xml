<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cbss.bean.canonico.Cargo" table="TCARGO" >
        <id name="codigoCargo" type="java.lang.Integer">
            <column name="CCARGO" />
        </id>
        <property name="descricaoCargo" type="java.lang.String">
            <column name="ICARGO" />
        </property>
        
        <property name="codigoExterno" type="java.lang.String">
            <column name="CEXTER" />
        </property>
    </class>
</hibernate-mapping>
