<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cbss.bean.canonico.FormaPagamento" table="TFORMAPGTO" >
        <id name="codigoFormaPagamento" type="java.lang.Long">
            <column name="CFORMAPGTO"  />
        </id>
        <property name="descricaoFormaPagamento" type="java.lang.String">
            <column name="RFORMAPGTO" />
        </property>
        <property name="codigoExterno" type="java.lang.String">
            <column name="CEXTER" />
        </property>

    </class>
</hibernate-mapping>

