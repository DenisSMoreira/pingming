<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cbss.bean.canonico.Banco" table="TBCO" >
        <id name="numeroBanco" type="java.lang.Long">
            <column name="NBCO" />
        </id>
        <property name="nomeBanco" type="java.lang.String">
            <column name="IBCO" />
        </property>
        <property name="codigoExterno" type="java.lang.Integer">
            <column name="CEXTER" />
        </property>
       
    </class>
</hibernate-mapping>
