<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cbss.bean.canonico.EntidadeDevedora" table="TENTDVDDORA" >
        <id name="codigoEntidadeDevedora" type="java.lang.Long">
            <column name="CENTDVDDORA" />
        </id>
        
        <many-to-one name="empresaParceira" class="cbss.bean.canonico.EmpresaParceira">
            <column name="CEMPRPCERA" />
        </many-to-one>
        <many-to-one name="banco" class="cbss.bean.canonico.Banco">
            <column name="NBCO"  />
        </many-to-one>
        <property name="codigoTipoEntidade" type="java.lang.Long">
            <column name="CTPOENTID" />
        </property>
       
    </class>
</hibernate-mapping>
