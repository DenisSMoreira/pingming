<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cbss.bean.canonico.ClientePF" table="TCLIPF" >
        <id name="codigo" type="java.lang.Long">
         <column name="TCLI"/>
        </id>
        <many-to-one name="atividadeProfissional" class="cbss.bean.canonico.AtividadeProfissional" >
            <column name="CATVDDPROFS"/>
        </many-to-one>
        <one-to-one name="cliente" class="cbss.bean.canonico.Cliente"  lazy="false" fetch="select"> </one-to-one>
        <property name="numeroCpf" type="java.lang.Long">
            <column name="NCPF"/>
        </property>
        <property name="dataNascimento" type="java.sql.Date">
            <column name="DNASC"/>
        </property>
        <property name="rg" type="java.lang.String">
            <column name="NRG"/>
        </property>
        <property name="numeroINSS" type="java.lang.Long">
            <column name="NINSS"/>
        </property>
    </class>
</hibernate-mapping>