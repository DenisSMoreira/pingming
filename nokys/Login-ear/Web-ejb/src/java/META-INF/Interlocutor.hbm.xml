<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cbss.bean.canonico.Interlocutor" table="TINTLOC" >
        <id name="codigoInterlocutor" type="java.lang.Long">
            <column name="CINTLOC" />
        </id>
        <many-to-one name="login" class="cbss.bean.canonico.Login"  lazy="false" fetch="select">
            <column name="NCPF"  />
        </many-to-one>
        <many-to-one name="cliente" class="cbss.bean.canonico.Cliente"  lazy="false" fetch="select">
            <column name="CCLI"  />
        </many-to-one>
        <many-to-one name="cargo" class="cbss.bean.canonico.Cargo"  lazy="false" fetch="select">
            <column name="CCARGO"  />
        </many-to-one>
        <property name="cpf" type="java.lang.Long" insert="false" update="false">
            <column name="NCPF" />
        </property>
        <property name="nomeInterlocutor" type="java.lang.String">
            <column name="IINTLOC" />
        </property>
        <property name="dataNascimento" type="java.sql.Date">
            <column name="DNASC" />
        </property>
        <property name="sexo" type="java.lang.Character">
            <column name="CSEXO" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="EEMAIL" />
        </property>
        <property name="telefonePrincipal" type="java.lang.Integer">
            <column name="NFONEPRINC" />
        </property>
        <property name="telefoneOpcional" type="java.lang.Integer">
            <column name="NFONEOPCNL" />
        </property>
        <property name="ramalPrincipal" type="java.lang.Integer">
            <column name="NRMALPRINC" />
        </property>
        <property name="ramalOpcional" type="java.lang.Integer">
            <column name="NRMALOPCNL" />
        </property>
        <property name="fax" type="java.lang.Integer">
            <column name="NFAX" />
        </property>
        <property name="departamento" type="java.lang.String">
            <column name="IDEPTO" />
        </property>
        <property name="ddd" type="java.lang.Integer">
            <column name="NDDDPRINC" />
        </property>
        <property name="ramalFax" type="java.lang.Integer">
            <column name="NRMALFAX" />
        </property>
        <property name="enderecoFilial" type="java.lang.Character">
            <column name="FENDERFLIAL" />
        </property>
        <property name="sincronizacao" type="java.lang.Boolean">
            <column name="FSINCR" />
        </property>
        <property name="dataCriacao" type="java.sql.Date">
            <column name="DCRIAC" />
        </property>
        <property name="dataAtualizacao" type="java.sql.Date">
            <column name="DATULZ" />
        </property>
        <property name="numeroDddOpcional" type="java.lang.Integer">
            <column name="NDDDOPCNL"  />
        </property>
        <property name="numeroDddFax" type="java.lang.Integer">
            <column name="NDDDFAX" />
        </property>
        <property name="exlusao" type="java.lang.Boolean">
            <column name="FEXCL"  />
        </property>
        <property name="endereco" type="java.lang.Long">
            <column name="CENDER"  />
        </property>
    </class>
</hibernate-mapping>
