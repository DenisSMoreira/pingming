<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cbss.bean.canonico.Termo" table="TTERMO">
       <id name="codigo" type="java.lang.Integer">
            <column name="CTERMO"/>
        </id>
        <property name="descricao" type="java.lang.String">
            <column name="RTERMO"/>
        </property>
        <property name="dataInicialVigenciaTermo" type="java.sql.Date">
            <column name="DINICVGCIATERMO"/>
        </property>
         <property name="codTipoTermo" type="java.lang.Byte">
            <column name="CTPOTERMO"/>
        </property>
    </class>
</hibernate-mapping>  