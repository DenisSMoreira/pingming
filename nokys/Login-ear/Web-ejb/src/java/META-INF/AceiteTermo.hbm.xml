<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cbss.bean.canonico.AceiteTermo" table="TACEITTERMO">
    <composite-id>
            <key-property name="numeroCpf" type="java.lang.Long">
                <column name="NCPF" />
            </key-property>
            <key-property name="codigoCliente" type="java.lang.Long">
                <column name="CCLI"  />
            </key-property>
            <key-property name="dataAceiteTermo" type="java.sql.Date">
                <column name="DACEITTERMO"  />
            </key-property>
            <key-property name="codigoTermo" type="java.lang.Integer">
               <column name="CTERMO"  />
            </key-property>
        </composite-id> 
    </class>
</hibernate-mapping>  